{
  "name": "@xsolare/trip-scheduler-scrapper",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "bun": ">=1.1.30"
  },
  "scripts": {
    "start": "bun run dist/run.js",
    "--------------------------------<      DEV       >--------------------------------": "",
    "dev": "bun run --hot --watch src/run.ts",
    "dev:debug": "bun --inspect run --hot --watch src/run.ts",
    "--------------------------------<      BUILD     >--------------------------------": "",
    "build:bun": "bun build ./src/index.ts --outdir ./dist --target bun --no-minify",
    "build": "tsup",
    "--------------------------------< LINT & FORMAT >---------------------------------": "",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "openai": "^6.3.0",
    "playwright": "^1.56.0",
    "prompts": "^2.4.2",
    "puppeteer": "^24.25.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "selenium-webdriver": "^4.36.0",
    "tsup": "^8.5.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.4.1",
    "@types/bun": "latest",
    "@types/cheerio": "^1.0.0",
    "@types/node": "^24.7.2",
    "@types/prompts": "^2.4.9",
    "@types/selenium-webdriver": "^4.35.1",
    "@types/uuid": "^11.0.0",
    "bun": "^1.2.23",
    "bun-types": "^1.2.23",
    "eslint": "9.36.0",
    "eslint-plugin-format": "1.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
